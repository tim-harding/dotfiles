{
  "arrow": {
    "body": "($1) => {$2}",
    "prefix": "arrow"
  },
  "component": {
    "body": [
      "<script setup lang=\"ts\">",
      "$1",
      "</script>",
      "",
      "<template>",
      "$2",
      "</template>",
      "",
      "<style module=\"s\">",
      "$3",
      "</style>"
    ],
    "prefix": "component"
  },
  "for": {
    "body": [
      "for ($1) {",
      "  $2",
      "}"
    ],
    "prefix": "for"
  },
  "function": {
    "body": [
      "function $1($2)$3 {",
      "  $4",
      "}"
    ],
    "prefix": "fn"
  },
  "grid-area": {
    "body": "grid-area: ${1:row-start} / ${2:col-start} / ${3:row-end} / ${4:col-end};",
    "prefix": "grid-area-4"
  },
  "if": {
    "body": [
      "if ($1) {",
      "  $2",
      "}"
    ],
    "prefix": "if"
  },
  "import_component": {
    "body": "import $1 from \"@/components/$1\";",
    "prefix": "import_component"
  },
  "interface": {
    "body": [
      "interface $1 {",
      "  $2",
      "}"
    ],
    "prefix": "interface"
  },
  "modimport": {
    "body": "import * as $1 from '@/logic/$1';",
    "prefix": "modimport"
  },
  "shadow": {
    "body": "${1:offset-x} ${2:offset-y} ${3:blur} ${4:spread} ${5:inset} ${6:color};",
    "prefix": "shadow"
  },
  "switch": {
    "body": [
      "switch ($1) {",
      "    $2",
      "}"
    ],
    "prefix": "switch"
  }
}
